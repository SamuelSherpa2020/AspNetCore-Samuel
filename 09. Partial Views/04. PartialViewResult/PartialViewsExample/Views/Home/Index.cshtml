@using PartialViewsExample.Models
<h1>Home</h1>

@* <partial name="_ListPartialView"></partial> *@

@{
 var myViewData = new ViewDataDictionary(ViewData);

 myViewData["ListTitle"] = "Countries";
 myViewData["ListItems"] = new List<string>() {
        "USA",
        "Canada",
        "Japan",
        "Germany",
        "India"
      };
}

@{
    ListModel listModel = new ListModel()
    {
        TitleName = "Human Name",
        ListItems = new List<string>()
        {
            "Samuel",
            "Rabin",
            "Subu",
            "Sahil",
            "Sapana",
            "Samu",
            "Prati"
        }
    };
}
<div class="box">
 <partial name="_ListPartialView" view-data="myViewData" model="listModel" ></partial>
</div>

<h3>ListTitle in View: @ViewData["ListTitle"]</h3>
